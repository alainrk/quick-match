    "coefficient",
    "levenshtein",
    "distance",
    "fast",
    "quickreply"
  ],
  "author": "Alain Di Chiappari <alain.devcs@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alainrk/quick-match/issues"
  },
  "homepage": "https://github.com/alainrk/quick-match#readme",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "pre-commit": "^1.2.2",
    "semver": "^7.1.0",
    "standard": "^16.0.1",
    "tap": "^15.0.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "standard": {
    "ignore": [
      "schema-validator.js"
    ]
  },
  "runkitExampleFilename": "example.js"
